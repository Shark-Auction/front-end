{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "types": ["node"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./components/*"],
      "@axios/*": ["./config/axios/*"],
      "@layout/*": ["./core/layout/*"],
      "@store/*": ["./core/store/*"],
      "@hooks/*": ["./hooks/*"],
      "@model/*": ["./model/*"],
      "@pages/*": ["./pages/*"],
      "@service/*": ["./service/*"],
      "@utils/*": ["./utils/*"]
    }
  }
}
